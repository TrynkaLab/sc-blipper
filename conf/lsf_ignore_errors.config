process {
    executor = 'lsf'
    maxRetries = 2
    errorStrategy = { task.attempt <= 2 ? "retry" : "ignore" }
    resourceLimits = [
        cpus: 64,
        memory: '512.GB',
        time: '12.h'
    ] 
    withLabel:process_long {
        resourceLimits = [
            cpus: 64,
            memory: '512.GB',
            time: '48.h'
        ] 
    }
}
executor {
    name = 'lsf'
    perJobMemLimit = true
    poolSize = 4
    submitRateLimit = '5 sec'
    killBatchSize = 40
    queueSize=40
}